
let {
  // These can also be environment variables
  $env_PORKBUN_KEY = "a_key"
  $env_PORKBUN_SECRET = "a_secret"

}  in {

  log_level = "debug"
  iface = "test0"

  ddns = {
    provider = {
      name = "porkbun"
      key = $env_PORKBUN_KEY
      secret = $env_PORKBUN_SECRET
    }

    domain = "example.com"
    host = "test"
  }
}
